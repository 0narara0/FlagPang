<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="8sp"
                android:color="#469939" />
            <corners
                android:radius="10sp" />
            <solid
                android:color="#9345983a" />
        </shape>
    </item>

</layer-list>